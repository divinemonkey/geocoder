{
  "name": "iso3166-1",
  "version": "0.2.8",
  "description": "Tiny, fast, modular ISO 3166-1 alpha-2/alpha-3 parser.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moimikey/iso3166-1.git"
  },
  "keywords": [
    "iso3166",
    "country",
    "countries",
    "iso",
    "locale"
  ],
  "author": {
    "name": "Michael Scott Hertzberg",
    "email": "mshertzberg@gmail.com",
